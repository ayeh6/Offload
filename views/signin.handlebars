<div id="signinBg">
  <div class="row bg-warning d-flex justify-content-center">
  <div class="col-12 m-5 d-flex justify-content-center">
    <img src="/assets/images/OffloadLogo.png" alt="Offload Logo">
  </div>
  <div class="col-12 d-flex justify-content-center">
    <form>
      <div class="mb-3">
        <label for="signinUsernameInput" class="form-label">Username</label>
        <input type="text" class="form-control" id="signinUsernameInput" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">Please enter your username</div>
      </div>
      <div class="mb-3">
        <label for="passwordInput" class="form-label">Password</label>
        <input type="password" class="form-control" id="signinPasswordInput">
      </div>
      <button type="submit" class="btn btn-primary" id="signinBtn">Sign In</button>
    </form>

  </div>
  <div class="col-12 m-5 d-flex justify-content-center">
    <a href="/signup">Create an Account</a>
  </div>
</div>
</div>




